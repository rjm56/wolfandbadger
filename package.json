{
  "name": "animal-top-trumps",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "lint": "eslint"
  },
  "dependencies": {
    "next": "9.3.4",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint": "^6.8.0",
    "jest": "^25.2.6",
    "jest-enzyme": "^7.1.2",
    "prettier": "^2.0.2"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "jest-enzyme",
      "<rootDir>/setupTests.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.next/"
    ],
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest"
    },
    "transformIgnorePatterns": [
      "/node_modules/",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    }
  }
}
